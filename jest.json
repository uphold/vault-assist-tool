{
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.js", "!**/node_modules/**"],
  "coverageReporters": ["text-summary", "json", "html"],
  "globals": {
    "__DEV__": true
  },
  "moduleFileExtensions": ["js", "json", "jsx", "node", "ts", "tsx", "png"],
  "moduleNameMapper": {
    "\\.(jpg|png)$": "<rootDir>/test/mocks/empty-asset.png",
    "\\.svg": "<rootDir>/test/mocks/svgr.js",
    "providers": "<rootDir>/test/mocks/providers.js",
    "^styled-components$": "<rootDir>/node_modules/styled-components/dist/styled-components.cjs.js",
    "^react-native$": "<rootDir>/node_modules/react-native-web",
    "^react$": "<rootDir>/node_modules/react/cjs/react.development.js",
    "react-native/Libraries/Renderer/shims": "<rootDir>/test/mocks/empty-module.js",
    "react-native-reanimated": "<rootDir>/node_modules/react-native-reanimated"
  },
  "setupFilesAfterEnv": ["<rootDir>/test/setup.js"],
  "testPathIgnorePatterns": [],
  "transformIgnorePatterns": [
    "node_modules/(?!(react-native-web|react-native-reanimated))"
  ],
  "testEnvironment": "jsdom",
  "verbose": true
}
